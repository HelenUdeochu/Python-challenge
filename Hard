x = 31
y = 4

def add():
  addition = x + y 
  print(f" {x} plus {y} equales {addition}")

add()

def substract()
 substraction = x - y
 print(f" {x} minus {y} equales {substraction}")

substract()

def multiply()
 multiplication = x * y
 print(f"{x} times {y} equales {multiplication}")

multiply()

def divide()
 division = x / y 
 print(f" {x} divided by {y} equales {division}")

divide()
